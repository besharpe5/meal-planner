{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist/client",
  "rewrites": [
    { "source": "/app(/.*)?", "destination": "/app" },
    { "source": "/login", "destination": "/" },
    { "source": "/register", "destination": "/" },
    { "source": "/about", "destination": "/" },
    { "source": "/privacy", "destination": "/" }
  ]
}
